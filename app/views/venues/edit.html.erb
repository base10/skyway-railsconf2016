<% @title = "Editing #{@venue.name}" %>

<%= render "form" %>

<br />
<p class="type">Merge "<%= @venue.name %>"</p>

<%= form_for [:merge, @venue] do |f| %>
  <%= f.label :other_venue_id, "Keep this venue and merge in:" %>
  <%= f.collection_select :other_venue_id, Venue.by_name, :id, :name %>
  <%= f.submit "Merge venue" %>
<% end %>
