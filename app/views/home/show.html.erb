<article>
<header>
  <h1>Aqueous</h1>
  <p>is a groove rock band</p>
</header>

<section>
  <h2>Tour</h2>
  <table>
    <% @upcoming_shows.each do |show| %>
      <tr>
        <td><%= performed_at_for(show) %></td>
        <td><%= show.venue.location %> @ <%= show.venue.name %></td>
      </tr>
    <% end %>
  </table>
</section>

<section>
  <h2>Listen</h2>
  <iframe id="bandcamp" style="border: 0; width: 100%;" src="http://bandcamp.com/EmbeddedPlayer/album=3634413396/size=large/bgcol=ffffff/linkcol=0687f5/transparent=true/" seamless><a href="http://aqueous1.bandcamp.com/album/willy-is-40">Willy is 40 by Aqueous</a></iframe>
</section>

<section>
  <h2>Watch</h2>
  <iframe id="youtube" width="100%" src="http://www.youtube.com/embed/x7YHjUm7XBI" frameborder="0" allowfullscreen></iframe>
</section>
</article>
